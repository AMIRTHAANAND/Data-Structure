import java.util.*;
public class Main{
public static void main(String[] args){
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int[] arr=new int[n];
        int s1=0;
        int s2=0;
        for(int i=0;i<n;i++){
            arr[i]=sc.nextInt();
            s1 +=arr[i];
            
        }
        s2=n*(n+1)/2;
            int result=s2-s1;
            System.out.println("The missing value is " + result);
}
}
